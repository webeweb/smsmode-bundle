{
    "name": "webeweb/smsmode-bundle",
    "type": "symfony-bundle",
    "description": "sMsmode bundle for Symfony 2 and more",
    "keywords": [
        "API",
        "bundle",
        "sMsmode",
        "SMS",
        "symfony",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/smsmode-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "webeweb/core-bundle": "^3.0",
        "webeweb/core-library": "^8.33",
        "webeweb/smsmode-library": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0",
        "monolog/monolog": "^1.18",
        "symfony/browser-kit": "^4.4|^5.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/security-bundle": "^4.4|^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\SMSModeBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\SMSModeBundle\\Tests\\": "Tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/smsmode-bundle/issues"
    }
}
